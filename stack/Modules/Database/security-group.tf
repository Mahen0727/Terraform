resource "aws_security_group" "rds" {
  name        = "Allow VPC - RDS Connections"
  description = "Allow VPC - RDS Connections"
  vpc_id      = "${}"

  ingress {
    from_port   = 0
    to_port     = 0
    protocol    = "-1"
    cidr_blocks = ["0.0.0.0/0"]
  }

  egress {
    from_port       = 0
    to_port         = 0
    protocol        = "-1"
    cidr_blocks     = ["0.0.0.0/0"]
    prefix_list_ids = ["pl-12c4e678"]
  }
}